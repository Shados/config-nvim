{
  "pyls": {
    "plugins": {
      "pycodestyle": {
        "enabled": false,
        "ignore": [
          "E501"
        ],
        "select": [
          "C",
          "E",
          "F",
          "W",
          "B",
          "B950"
        ]
      }
    },
    "configurationSources": [
      "pycodestyle",
      "flake8"
    ]
  },
  "solargraph": {
    "diagnostics": true
  }
}
